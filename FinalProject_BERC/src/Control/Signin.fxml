<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="525.0" prefWidth="878.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control.SigninController">
   <children>
      <TextField fx:id="emailtextfield" layoutX="315.0" layoutY="101.0" promptText="Enter email" />
      <TextField fx:id="passwordtextfield" layoutX="315.0" layoutY="155.0" promptText="Enter Password" />
      <ComboBox fx:id="signupCombobox" layoutX="324.0" layoutY="211.0" prefWidth="150.0" />
      <Button layoutX="366.0" layoutY="263.0" mnemonicParsing="false" onAction="#signupButtonOnClick" text="Sign In" />
      <Button layoutX="563.0" layoutY="408.0" mnemonicParsing="false" onAction="#signinOnClick" text="New user! Register here" />
      <Button layoutX="366.0" layoutY="338.0" mnemonicParsing="false" onAction="#changebuttonOnClick" text="Change Password" />
   </children>
</AnchorPane>
